<h1>Search Author</h1>

<form action="/authors" method="GET">
    <label for="author">Author:</label>
    <input type="text" name="name" value="<%= searchOptions.name %>">
    <button type="submit">Search</button>
    
    <br><br>

    <% authors.forEach(author => { %>
        <div><%= author.name %></div>
    <% }); %>
</form>